<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!--监测方式1周期 2时间点-->
    <add key="RunType" value="2" />
    <!--统计时间 优先以当前时间为准每周新七天凌晨2点-->
    <add key="CountRunHours" value="02:00:00" />

    <!--统计周期 单位分钟-->
    <add key="CountRunCycle" value="60" />
  </appSettings>
</configuration>